QMainWindow, minimap_panel {
  background-image:url(LittleFingerpattern.png);
}

*[doomed="true"] {
  background-color: #e1cea1;
  border: none;
}

*[shortcuts="true"] {
  background-color: #e1cea1;
  border: none;
}

*[city_name_button="true"] {
  font-weight: bold italic;
  border: 0px;
  background: transparent;
  text-align: right;
}

/* color of border only - set the same as background */
*[messagewindow="true"] {
  color: #120c06;
}

/* color of border only*/
*[top_bar="true"] {
  color: #000000;
}

/* Help dialog tile output 3 labels */
*[foodlab="true"] {
  background:#00ee01;
}

*[shieldlab="true"] {
  background: #e4e4e4;
}

*[ecolab="true"] {
  background: #eeee12;
}

*[doomchat="true"] {
  background-color: white;
  border: 1px solid #c3b48e;
}

*[themed_choice="true"] {
  color: #000000;
  background-color: #e7d6b0;
  border: 2px solid #c3b48e;
}

*[unit_wait_table="true"] {
  background-color: rgb(15,10,5);
  border: 1px solid #685f47;
  color: white;
}

*[civstatus_bg="true"] {
   background-color: rgba(0,0,0,125);
}

*[civstatus="true"] {
  color: white;
}

