/* This header is generated from gen_headers/freeciv_config.h.in.
 * It contains values to specific freeciv build, but only such
 * values that should not conflict with configuration headers from
 * other autotools projects. This is meant to be usable for external
 * projects building against some freeciv components. */

#ifndef FC__FREECIV_CONFIG_H
#define FC__FREECIV_CONFIG_H

/* Is this freeciv-web instead of regular build */
#cmakedefine FREECIV_WEB

/* Testmatic integration enabled */
#cmakedefine FREECIV_TESTMATIC

/* Metaserver URL */
#cmakedefine FREECIV_META_URL "${FREECIV_META_URL}"

/* Month next version will be released */
#cmakedefine FREECIV_RELEASE_MONTH

/* Use C11 threads as thread implementation */
#cmakedefine FREECIV_HAVE_C11_THREADS

/* Use pthreads as thread implementation */
#cmakedefine FREECIV_HAVE_PTHREAD

/* Use Windows threads as thread implementation */
#cmakedefine FREECIV_HAVE_WINTHREADS

/* Use tinycthread as thread implementation */
#cmakedefine FREECIV_HAVE_TINYCTHR

/* Has thread condition variable implementation */
#cmakedefine FREECIV_HAVE_THREAD_COND

/* Build xml-backend for registry */
#cmakedefine FREECIV_HAVE_XML_REGISTRY

/* Native language support enabled */
#cmakedefine FREECIV_ENABLE_NLS

/* libintl.h available */
#cmakedefine FREECIV_HAVE_LIBINTL_H

/* sys/types.h available */
#cmakedefine FREECIV_HAVE_SYS_TYPES_H

/* unistd.h available */
#cmakedefine FREECIV_HAVE_UNISTD_H

/* sys/time.h available */
#cmakedefine FREECIV_HAVE_SYS_TIME_H

/* dirent.h available */
#cmakedefine FREECIV_HAVE_DIRENT_H

/* Readline support */
#cmakedefine FREECIV_HAVE_LIBREADLINE

/* Windows build */
#cmakedefine FREECIV_MSWINDOWS 1

/* socket zero isn't same stdin */
#cmakedefine FREECIV_SOCKET_ZERO_NOT_STDIN

/* Extra debugging support */
#cmakedefine FREECIV_DEBUG

/* No freeciv specific debugging support at all */
#cmakedefine FREECIV_NDEBUG

/* zlib is available */
#cmakedefine FREECIV_HAVE_LIBZ

/* libbzip2 is available */
#cmakedefine FREECIV_HAVE_LIBBZ2

/* liblzma is available */
#cmakedefine FREECIV_HAVE_LIBLZMA

/* Location for freeciv to store its information */
#cmakedefine FREECIV_STORAGE_DIR "${FREECIV_STORAGE_DIR}"

/* Max number of AI modules */
#cmakedefine FREECIV_AI_MOD_LAST ${FREECIV_AI_MOD_LAST}

/* C11 static assert supported */
#cmakedefine FREECIV_C11_STATIC_ASSERT

/* strlen() in static assert supported */
#cmakedefine FREECIV_STATIC_STRLEN

/* C++11 static assert supported */
#cmakedefine FREECIV_CXX11_STATIC_ASSERT

/* opendir() available */
#cmakedefine FREECIV_HAVE_OPENDIR

#endif /* FC__FREECIV_CONFIG_H */
