#: data/civ2civ3/terrain.ruleset:1875 data/classic/terrain.ruleset:1667
#: data/multiplayer/terrain.ruleset:1639 data/sandbox/terrain.ruleset:1915
msgid "Coal"
msgstr "Węgiel"

#: data/civ1/terrain.ruleset:1226 data/civ2/terrain.ruleset:1406
#: data/civ2civ3/terrain.ruleset:1914 data/classic/terrain.ruleset:1706
#: data/multiplayer/terrain.ruleset:1678 data/sandbox/game.ruleset:2045
#: data/sandbox/terrain.ruleset:1954
msgid "Gems"
msgstr "Klejnoty"

#: data/civ1/terrain.ruleset:1239
msgid "Horses"
msgstr "Konie"

#: data/civ1/terrain.ruleset:1252 data/civ2/terrain.ruleset:1445
#: data/civ2civ3/terrain.ruleset:1953 data/classic/terrain.ruleset:1745
#: data/multiplayer/terrain.ruleset:1717 data/sandbox/terrain.ruleset:1993
msgid "Oasis"
msgstr "Oaza"

