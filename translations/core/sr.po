msgstr ""

#: data/experimental/units.ruleset:531 data/experimental/units.ruleset:577
msgid "?worker_level:journeyman"
msgstr ""

#: data/experimental/units.ruleset:533 data/experimental/units.ruleset:579
msgid "?worker_level:master"
